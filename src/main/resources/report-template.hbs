<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <title>Maven Profiler report</title>
</head>
<body>
    <section>
        <h1>{{name}}</h1>
        <p>Run on: {{date}}</p>

        {{#projects}}
        <article>
            <h1>{{name}} - {{time}}</h1>
            <table>
                <thead>
                <th>Plugin execution</th>
                <th>Duration</th>
                </thead>
                <tbody>
                {{#mojosWithTime}}
                <tr>
                    <td>{{entry}}</td>
                    <td>{{time}}</td>
                </tr>
                {{/mojosWithTime}}
                </tbody>
            </table>
        </article>
        {{/projects}}

        {{#downloadSectionDisplayed}}
        <article>
            <h1>Artifact Downloading</h1>
            <table>
                <thead>
                <th>Artifact</th>
                <th>Duration</th>
                </thead>
                <tbody>
                {{#downloads}}
                    <tr>
                        <td>{{entry}}</td>
                        <td>{{time}}</td>
                    </tr>
                {{/downloads}}
                </tbody>
            </table>
        </article>
        {{/downloadSectionDisplayed}}
    </section>

    <footer>Generated by <a href="https://github.com/jcgay/maven-profiler">maven-profiler</a>.</footer>
</body>
</html>